[Unit]
Description=VibeDownloader Service
After=network.target

[Service]
User=root
WorkingDirectory=/root/VibeDownloader
Environment="PATH=/root/VibeDownloader/venv/bin"
# ADICIONADO --timeout 300 PARA N√ÉO CAIR O DOWNLOAD
ExecStart=/root/VibeDownloader/venv/bin/gunicorn --workers 3 --bind 0.0.0.0:5002 --timeout 300 app:app
Restart=always

[Install]
WantedBy=multi-user.target